@page
@model BrekkenScan.Web.Pages.Account.RegisterModel
@{
}

<div class="login-wrapper">
    <h2>Registrer</h2>
    <form method="post">
        <div class="form-group">
            <label>Brukernavn</label>
            <input class="form-control" asp-for="Name" type="text" required />
            <span class="text-danger" asp-validation-for="Name"></span>
        </div>
        <div class="form-group">
            <label>Passord</label>
            <input class="form-control" asp-for="Password" type="password" required />
            <span class="text-danger" asp-validation-for="Password"></span>
        </div>
        <input class="btn btn-success" value="Registrer" type="submit" />
    </form>
</div>
